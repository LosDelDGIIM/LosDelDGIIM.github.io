                <stringProp name="Header.value">Bearer ${token}</stringProp>
              </elementProp>
            </collectionProp>
            <stringProp name="TestPlan.comments">AÃ±adir el Token en la Header</stringProp>
          </HeaderManager>
          <hashTree/>
        </hashTree>
